{
  "name": "pixelmord-github-page",
  "description": "Personal website of Andreas Adam",
  "version": "1.0.0",
  "author": "Andreas Adam",
  "dependencies": {
    "@theme-ui/color": "^0.3.1",
    "date-fns": "^2.15.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "next": "^9.5.2",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "prestyled": "^0.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-particles-js": "^3.3.0",
    "responsive-loader": "^2.0.2",
    "sharp": "^0.25.4",
    "theme-ui": "^0.3.1",
    "typeface-lato": "^0.0.75"
  },
  "resolutions": {
    "@babel/core": "7.11.4"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/polyfill": "^7.10.4",
    "@pacote/eslint-config": "^3.0.1",
    "@pacote/eslint-config-jest": "^1.1.0",
    "@pacote/eslint-config-react": "^1.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/babel__core": "^7.1.9",
    "@types/core-js": "^2.5.3",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/node": "^14.6.0",
    "@types/prettier": "^2.0.2",
    "@types/prismjs": "^1.16.1",
    "@types/ramda": "^0.27.14",
    "@types/react": "^16.9.47",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@types/testing-library__react": "^10.2.0",
    "@types/theme-ui": "^0.3.6",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.1.7",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.12",
    "prettier": "^2.1.0",
    "ts-jest": "^26.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  }
}
